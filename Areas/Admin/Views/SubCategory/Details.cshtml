@model Roadsters.Models.SubCategory

<br />
<h2 class="text-light">
    SubCategory Details
</h2>
<br />
<div class="backgroundColor">
    <fieldset disabled>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Name" class="custom-label text-white"></label>
            </div>
            <div class="col-5">
                <input asp-for="Name" class="form-control" style="background-color:white" />
            </div>
        </div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="Category.Name" class="custom-label text-white"></label>
            </div>
            <div class="col-5">
                <input asp-for="Category.Name" class="form-control" text-white style="background-color:white" />
            </div>
        </div>
    </fieldset>
    <br />
    <div class="form-group row">
        <div class="col-5 offset-2">
            <partial name="_DetailAndBackButton" model="Model.Id" />
        </div>
    </div>
</div>